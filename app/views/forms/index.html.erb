  <h2 align="center">Dashboard</h2>
<div id="response" class="container tab-pane active"><br>
  <div class="row h-100 justify-content-center align-items-center">
    <table class="table">
      <thead>
        <tr>
          <th>#</th>
          <th >Questionnaire Form</th>
          <th>Actions</th>
        </tr>
      </thead>
      <tbody>
        <% i = 0%>
        <% @forms.each do |form| %>
          <% i += 1 %>
          <tr>
            <td><%= i%></td>
           <td><%= form.form_title %></td>
           <td><%= link_to 'Response', form %>
            <%= link_to 'Preview', preview_forms_path(id: form) %>
            <%= link_to 'Edit', edit_form_path(form) %>
            <%= link_to 'Destroy', form, method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>


